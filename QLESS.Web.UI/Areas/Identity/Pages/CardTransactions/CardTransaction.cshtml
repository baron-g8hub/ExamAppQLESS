@page
@using QLESS.Web.UI
@model CardTransactionModel
@{
    ViewData["Title"] = "Card Transaction";
}




<div class="row g-3">
    <div class="col">
        <input type="text" class="form-control" placeholder="First name" aria-label="First name">
    </div>
    <div class="col">
        <input type="text" class="form-control" placeholder="Last name" aria-label="Last name">
    </div>
</div>


<div class="row">
    <div class="col-md-4">
        <form id="registerForm" asp-route-returnUrl="@Model.ReturnUrl" method="post">
            <h2>Create a new account.</h2>
            <hr />
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-floating">
                <label asp-for="Input.AmountTotal"></label>
            </div>
            <div class="form-floating">
                <input asp-for="Input.AmountReceived" class="form-control" autocomplete="amountreceived" aria-required="true" />
                <label asp-for="Input.AmountReceived"></label>
                <span asp-validation-for="Input.AmountReceived" class="text-danger"></span>
            </div>
         @*   <div class="form-floating">
                <input asp-for="Input.EmployeeNumber" class="form-control" autocomplete="employeenumber" aria-required="true" />
                <label asp-for="Input.EmployeeNumber"></label>
                <span asp-validation-for="Input.EmployeeNumber" class="text-danger"></span>
            </div>*@
       
            <button id="registerSubmit" type="submit" class="w-100 btn btn-lg btn-primary">Register</button>
        </form>
    </div>
</div>




@section Scripts {
<partial name="_ValidationScriptsPartial" />
}
